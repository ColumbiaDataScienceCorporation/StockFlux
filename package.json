{
  "name": "stockflux",
  "version": "13.0.0",
  "devDependencies": {
    "lerna": "^3.16.4"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --concurrency 1",
    "build": "lerna run build",
    "start-chart": "cd packages/stockflux-chart && npm run start",
    "start-container": "cd packages/stockflux-container && npm run start",
    "start-launcher": "cd packages/stockflux-launcher && npm run start",
    "start-news": "cd packages/stockflux-news && npm run start",
    "start-watchlist": "cd packages/stockflux-watchlist && npm run start",
    "launch-chart": "cd packages/stockflux-chart && npm run launch",
    "launch-launcher": "cd packages/stockflux-launcher && npm run launch",
    "launch-news": "cd packages/stockflux-news && npm run launch",
    "launch-watchlist": "cd packages/stockflux-watchlist && npm run launch"
  }
}
