{
    "runtime": {
      "arguments": "--enable-aggressive-domstorage-flushing",
      "version": "9.61.38.40"
    },
    "startup_app": {
      "name": "stockflux-chart-container",
      "uuid": "stockflux-chart-container",
      "url": "http://localhost:8051/index.html",
      "applicationIcon": "{fqdn}/artifacts/stockflux-chart/favicon.ico",
      "autoShow": true,
      "defaultWidth": 1280,
      "defaultHeight": 720,
      "minWidth": 1000,
      "minHeight": 600,
      "frame": false,
      "contextMenu": true,
      "customData": {
        "apiBaseUrl": "https://lf467ndb08.execute-api.eu-west-2.amazonaws.com/dev/api"
      }
    },
    "services": [
      {
        "name": "layouts",
        "config": {
          "features": {
            "tab": false
          }
        }
      }
    ],
    "shortcut": {
      "company": "ScottLogic",
      "description": "Hosts StockFlux Chart as an OpenFin application.",
      "name": "StockFlux Chart",
      "icon": "{fqdn}/artifacts/stockflux-chart/v0.0.1/favicon.ico"
    },
    "splashScreenImage": "{fqdn}/artifacts/stockflux-chart/v0.0.1/splashscreen.png"
}